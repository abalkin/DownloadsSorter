{
  "name": "DownloadsSorter",
  "description": "Save downloaded files into datestamped folders.",
  "version": "0.0.1",
  "minimum_chrome_version": "30.0.1599",
  "icons": { 
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png" },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "downloads"
  ],
  "manifest_version": 2
}
